SELECT *
  FROM users;

SELECT *
  FROM users
  WHERE lastname = 'Smith'
  OR age >= 21;

SELECT userid,firstname,lastname,users.userTypeId,userType
  FROM users
  JOIN userType ON users.userTypeId = userType.userTypeId
  WHERE age < 25;

